{
  "name": "project-startup",
  "version": "0.0.1",
  "description": "A project demo for startup",
  "main": "index.js",
  "scripts": {
    "clean": "node ./build/clean.js",
    "dev": "cross-env BUILD_ENV=dev webpack-dev-server --progress --config ./build/webpack.config.dev.js",
    "build:dev": "npm run clean && cross-env BUILD_ENV=dev webpack --display-optimization-bailout --config ./build/webpack.config.dist.js",
    "build:test": "npm run clean && cross-env BUILD_ENV=test webpack --display-optimization-bailout --config ./build/webpack.config.dist.js",
    "build:prev": "npm run clean && cross-env BUILD_ENV=prev webpack --display-optimization-bailout --config ./build/webpack.config.dist.js",
    "build:prod": "npm run clean && cross-env BUILD_ENV=production webpack --display-optimization-bailout --config ./build/webpack.config.dist.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KyLeoHC/project-startup.git"
  },
  "keywords": [
    "webpack",
    "vue",
    "demo",
    "startup",
    "project"
  ],
  "author": "KyLeo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KyLeoHC/project-startup/issues"
  },
  "homepage": "https://github.com/KyLeoHC/project-startup#readme",
  "devDependencies": {
    "autoprefixer": "^9.0.2",
    "axios": "^0.18.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "css": "^2.2.3",
    "css-loader": "^1.0.0",
    "es6-promise": "^4.2.4",
    "eslint": "^5.2.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "eslint-plugin-vue": "^5.0.0-beta.1",
    "glob": "^7.1.2",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.1",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss-loader": "^2.1.6",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue": "^2.5.16",
    "vue-loader": "^15.2.6",
    "vue-router": "^3.0.1",
    "vue-style-loader": "^4.1.1",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
