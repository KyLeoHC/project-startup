{
  "name": "project-startup",
  "version": "0.0.1",
  "description": "A project demo for startup",
  "main": "index.js",
  "scripts": {
    "clean": "node ./build/clean.js",
    "dev": "cross-env BUILD_ENV=dev webpack-dev-server --progress --config ./build/webpack.config.dev.js",
    "build:dev": "npm run clean && cross-env BUILD_ENV=dev webpack --display-optimization-bailout --config ./build/webpack.config.dist.js",
    "build:test": "npm run clean && cross-env BUILD_ENV=test webpack --display-optimization-bailout --config ./build/webpack.config.dist.js",
    "build:prev": "npm run clean && cross-env BUILD_ENV=prev webpack --display-optimization-bailout --config ./build/webpack.config.dist.js",
    "build:prod": "npm run clean && cross-env BUILD_ENV=production webpack --display-optimization-bailout --config ./build/webpack.config.dist.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KyLeoHC/project-startup.git"
  },
  "keywords": [
    "webpack",
    "vue",
    "demo",
    "startup",
    "project"
  ],
  "author": "KyLeo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KyLeoHC/project-startup/issues"
  },
  "homepage": "https://github.com/KyLeoHC/project-startup#readme",
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/runtime": "^7.1.2",
    "@babel/runtime-corejs2": "^7.1.2",
    "autoprefixer": "^9.0.2",
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "css": "^2.2.3",
    "css-loader": "^1.0.0",
    "eslint": "^5.6.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.0.0-beta.3",
    "glob": "^7.1.2",
    "html-webpack-plugin": "^3.2.0",
    "inline-source-webpack-plugin": "^1.0.1",
    "mini-css-extract-plugin": "^0.4.3",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss-loader": "^3.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue": "^2.5.16",
    "vue-loader": "^15.4.2",
    "vue-router": "^3.0.1",
    "vue-style-loader": "^4.1.1",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
